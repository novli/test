{
  "cacheDirectory": "cache/jest",
  "globals": {
    "BABEL_ENV": "test",
    "NODE_ENV": "production"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleNameMapper": {
    "^.+\\.(sss)$": "identity-obj-proxy"
  },
  "setupFiles": [
    "./jest.setup.js"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "transform": {
    "^.+\\.(js|jsx)$": "<rootDir>/__transformers__/babel-workerize.js"
  },
  "verbose": true
}
