{
  "extends": "airbnb",
  "rules": {
      "arrow-body-style": ["error", "as-needed"],
      "arrow-parens": ["error", "as-needed"],
      "no-debugger": "warn"
  },
  "env": {
      "browser": true,
      "jest": true
  },
  "settings": {
      "import/resolver": {
          "babel-module": {}
      }
  }
}